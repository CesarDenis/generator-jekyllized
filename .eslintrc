{
  "parser": "babel-eslint",
  "globals": {
    "describe": true,
    "before": true,
    "it": true
  },
  "rules": {
    "strict": [2, "global"],
    "indent": [2, 2],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "space-before-function-paren": [2, {anonymous: "never", named: "never"}]
  },
  "env": {
      "es6": true,
      "node": true,
      "mocha": true
  },
  "extends": "xo"
}
