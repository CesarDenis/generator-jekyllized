{
  "name": "generator-jekyllized",
  "version": "1.0.0-beta.21",
  "main": "index.js",
  "description": "Use Jekyll with Gulp: Sass, sourcemaps, AutoPrefixer, asset optimization and cache busting and much, much more. Can you really want more?",
  "keywords": [
    "yeoman-generator",
    "jekyll",
    "static",
    "gulp"
  ],
  "author": "Sondre Nilsen",
  "repository": "sondr3/generator-jekyllized",
  "license": "MIT",
  "scripts": {
    "test": "xo && nyc ava"
  },
  "files": [
    "index.js",
    "generators"
  ],
  "dependencies": {
    "chalk": "^1.1.0",
    "lodash": "^4.11.1",
    "shelljs": "^0.7.0",
    "yargs": "^4.6.0",
    "yeoman-generator": "^0.23.3"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "coveralls": "^2.11.9",
    "nyc": "^6.4.4",
    "xo": "^0.15.1",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.1.0",
    "yo": "^1.7.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "new-cap": 0
    },
    "ignores": [
      "generators/app/templates/**",
      "generators/boilerplate/templates/**",
      "generators/gulp/templates/**",
      "generators/jekyll/templates/**"
    ]
  }
}
